{
    "$schema": "https://json.schemastore.org/template",
    "author": "Vijay Anand E G",
    "defaultName": "UserControl1",
    "classifications": [
        "UserControl",
        "Windows",
        "WinUI",
        "Xaml"
    ],
    "identity": "VijayAnand.WinUIUserControl",
    "groupIdentity": "VijayAnand.WinUITemplates.UserControl.Xaml",
    "description": "An item template for WinUI 3 UserControl in XAML",
    "name": "WinUI 3 UserControl",
    "shortName": "winui-usercontrol",
    "sourceName": "WinUIUserControl.1",
    "primaryOutputs": [
        {
            "path": "WinUIUserControl.1.xaml"
        },
        {
            "path": "WinUIUserControl.1.xaml.cs"
        }
    ],
    "tags": {
        "language": "C#",
        "type": "item"
    },
    "symbols": {
        "namespace": {
            "type": "bind",
            "binding": "msbuild:RootNamespace",
            "replaces": "MyApp.Namespace",
            "defaultValue": "MyApp.Namespace"
        }
    },
    "constraints": {
        "dotnet7-sts": {
            "type": "sdk-version",
            "args": [
                "[7.0,)"
            ]
        },
        "csharp-only": {
            "type": "project-capability",
            "args": "CSharp"
        }
    }
}

<?xml version="1.0" encoding="UTF-8" ?>
<!--#if (Mvvm)-->
<!--#if (ImplicitNamespace)-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="{Binding Title}"
    x:DataType="MauiViewModel"
    ControlTemplate="{StaticResource VersionTemplate}">
    <ContentPage.BindingContext>
        <MauiViewModel />
    </ContentPage.BindingContext>
<!--#elif (GlobalNamespace)-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns="http://schemas.microsoft.com/dotnet/maui/global"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/maui/global/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="{Binding Title}"
    x:DataType="MauiViewModel"
    ControlTemplate="{StaticResource VersionTemplate}"
    mc:Ignorable="d">
    <ContentPage.BindingContext>
        <MauiViewModel />
    </ContentPage.BindingContext>
<!--#else-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:b="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:local="clr-namespace:WinUIBlazorApp._1"
    xmlns:vm="clr-namespace:WinUIBlazorApp._1.ViewModels"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="{Binding Title}"
    x:DataType="vm:MauiViewModel"
    ControlTemplate="{StaticResource VersionTemplate}"
    mc:Ignorable="d">
    <ContentPage.BindingContext>
        <vm:MauiViewModel />
    </ContentPage.BindingContext>
<!--#endif-->
<!--#else-->
<!--#if (ImplicitNamespace)-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    ControlTemplate="{StaticResource VersionTemplate}">
<!--#elif (GlobalNamespace)-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns="http://schemas.microsoft.com/dotnet/maui/global"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:d="http://schemas.microsoft.com/dotnet/maui/global/design"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    ControlTemplate="{StaticResource VersionTemplate}"
    mc:Ignorable="d">
<!--#else-->
<ContentPage
    x:Class="WinUIBlazorApp._1.Views.MauiPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:b="clr-namespace:Microsoft.AspNetCore.Components.WebView.Maui;assembly=Microsoft.AspNetCore.Components.WebView.Maui"
    xmlns:d="http://schemas.microsoft.com/dotnet/2021/maui/design"
    xmlns:local="clr-namespace:WinUIBlazorApp._1"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    ControlTemplate="{StaticResource VersionTemplate}"
    mc:Ignorable="d">
<!--#endif-->
<!--#endif-->
    <ContentPage.Content>
        <!--#if (GlobalNamespace || ImplicitNamespace)-->
        <!--#if (Mvvm)-->
        <BlazorWebView
            HostPage="wwwroot/index.html"
            StartPath="{Binding StartPath}">
        <!--#else-->
        <BlazorWebView
            HostPage="wwwroot/index.html"
            StartPath="/counter">
        <!--#endif-->
            <BlazorWebView.RootComponents>
                <RootComponent
                    ComponentType="{x:Type Main}"
                    Selector="#app" />
            </BlazorWebView.RootComponents>
        </BlazorWebView>
        <!--#else-->
        <!--#if (Mvvm)-->
        <b:BlazorWebView
            HostPage="wwwroot/index.html"
            StartPath="{Binding StartPath}">
        <!--#else-->
        <b:BlazorWebView
            HostPage="wwwroot/index.html"
            StartPath="/counter">
        <!--#endif-->
            <b:BlazorWebView.RootComponents>
                <b:RootComponent
                    ComponentType="{x:Type local:Main}"
                    Selector="#app" />
            </b:BlazorWebView.RootComponents>
        </b:BlazorWebView>
        <!--#endif-->
    </ContentPage.Content>
</ContentPage>
